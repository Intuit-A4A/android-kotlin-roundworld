<?xml version="1.0" encoding="utf-8"?>

<!-- Network configuration changes for doing Charles Proxy debugging.
     Should be used only in debugging environments -->
<network-security-config>
    <debug-overrides>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </debug-overrides>
</network-security-config>